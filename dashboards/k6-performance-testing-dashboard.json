{
  "id": null,
  "uid": null,
  "title": "k6 Performance Testing Dashboard",
  "tags": ["k6", "performance", "load-testing"],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 0,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "HTTP Request Rate",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "rate(k6_http_reqs[1m])",
          "legendFormat": "Requests/sec",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "reqps", "label": "Requests/sec"},
        {"format": "short"}
      ]
    },
    {
      "id": 2,
      "title": "HTTP Request Duration (p90)",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "k6_http_req_duration{quantile=\"0.9\"}",
          "legendFormat": "p90",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "s", "label": "Duration"},
        {"format": "short"}
      ]
    },
    {
      "id": 3,
      "title": "HTTP Request Duration (p99)",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "k6_http_req_duration{quantile=\"0.99\"}",
          "legendFormat": "p99",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "s", "label": "Duration"},
        {"format": "short"}
      ]
    },
    {
      "id": 4,
      "title": "HTTP Request Failure Rate",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "rate(k6_http_req_failed[1m])",
          "legendFormat": "Failed Requests/sec",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "reqps", "label": "Failed Requests/sec"},
        {"format": "short"}
      ]
    },
    {
      "id": 5,
      "title": "Virtual Users (VUs)",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "k6_vus",
          "legendFormat": "VUs",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "short", "label": "Virtual Users"},
        {"format": "short"}
      ]
    },
    {
      "id": 6,
      "title": "Data Transferred",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "targets": [
        {
          "expr": "rate(k6_data_received[1m])",
          "legendFormat": "Received",
          "refId": "A"
        },
        {
          "expr": "rate(k6_data_sent[1m])",
          "legendFormat": "Sent",
          "refId": "B"
        }
      ],
      "yaxes": [
        {"format": "bytes", "label": "Bytes/sec"},
        {"format": "short"}
      ]
    },
    {
      "id": 7,
      "title": "Error Rate Percentage",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24},
      "targets": [
        {
          "expr": "rate(k6_http_req_failed[5m]) / rate(k6_http_reqs[5m]) * 100",
          "refId": "A"
        }
      ],
      "options": {
        "unit": "percent",
        "decimals": 2
      }
    },
    {
      "id": 8,
      "title": "Total Requests",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 24},
      "targets": [
        {
          "expr": "sum(increase(k6_http_reqs[1h]))",
          "refId": "A"
        }
      ],
      "options": {
        "unit": "short"
      }
    },
    {
      "id": 9,
      "title": "Average Response Time",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 24},
      "targets": [
        {
          "expr": "k6_http_req_duration{quantile=\"0.5\"}",
          "refId": "A"
        }
      ],
      "options": {
        "unit": "s",
        "decimals": 3
      }
    },
    {
      "id": 10,
      "title": "Current Test Type",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 24},
      "targets": [
        {
          "expr": "count(k6_http_reqs) by (test_type)",
          "refId": "A"
        }
      ],
      "options": {
        "unit": "none"
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "links": [],
  "annotations": {
    "list": []
  },
  "templating": {
    "list": []
  }
}
